<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.mapper.StuBatchMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.beans.StuBatch">
        <id column="batch_id" property="batchId" />
        <result column="batch_name" property="batchName" />
        <result column="batch_start_time" property="batchStartTime" />
        <result column="batch_end_time" property="batchEndTime" />
        <result column="batch_credit" property="batchCredit" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        batch_id, batch_name, batch_start_time, batch_end_time, batch_credit
    </sql>

</mapper>
